{
  "name": "fandom-react-boilerplate",
  "version": "1.0.0",
  "description": "A reasonable starting point for a React application",
  "repository": "https://github.com/gbenson-wikia/fandom-react-boilerplate.git",
  "author": "Geoffrey Benson <gbenson@wikia-inc.com>",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "container:build": "../scripts/host_only.sh && ../scripts/build_container.sh",
    "container:start": "../scripts/host_only.sh && ../scripts/start_container.sh",
    "dev": "../scripts/container_only.sh && npm-run-all --parallel dev-client dev-server",
    "dev-client": "../scripts/container_only.sh && webpack-serve webpack/webpack-dev-client.conf.js",
    "dev-server": "../scripts/container_only.sh && cross-env NODE_ENV=development webpack --progress --colors --config webpack/webpack-dev-server.conf.js",
    "clean": "rimraf build",
    "start:server": "../scripts/container_only.sh && webpack --config webpack/webpack-dev-server.conf.js",
    "start:client": "../scripts/container_only.sh && webpack-serve webpack/webpack-dev-client.conf.js",
    "start": "yarn clean && npm-run-all --parallel start:server start:client",
    "lint:eslint": "yarn eslint --ext js client server",
    "lint:fix:eslint": "yarn eslint --fix --ext js client server",
    "lint:stylelint": "yarn stylelint \"client/**/*.{css,scss}\"",
    "lint:fix:stylelint": "yarn stylelint --fix \"client/**/*.{css,scss}\"",
    "lint": "npm-run-all --continue-on-error --parallel lint:eslint lint:stylelint",
    "lint:fix": "npm-run-all --continue-on-error --parallel lint:fix:eslint lint:fix:stylelint"
  },
  "dependencies": {
    "@wikia/react-design-system": "git://github.com/Wikia/react-design-system.git#2.2.3",
    "axios": "^0.18.0",
    "css-loader": "^0.28.11",
    "design-system": "git://github.com/Wikia/design-system.git#14.2.0",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "mime": "^2.3.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "rimraf": "^2.6.2",
    "start-server-webpack-plugin": "^2.2.5",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.3",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.4"
  }
}
